hierarchy.costcentertype_0 = 'BUDGET')\n\t\tTHEN bv_box_hierarchy.id\n\t\tELSE substring(bv_box_well.budgetareaid_0, 2, len(bv_box_well.budgetareaid_0))\n\t\tEND AS budget_area_id,\n\tCASE WHEN (bv_box_hierarchy.costcentertype_0 = 'BUDGET')\n\t\tTHEN bv_box_hierarchy.description\n\t\tELSE bv_box_well.budgetarea_0\n\t\tEND AS budget_area, bv_box_hierarchy.nodeid_0 AS node_id,\n\tbv_box_hierarchy.node AS node,\n\tbv_box_hierarchy.nodedepth_0 AS node_depth,\n\tbv_box_hierarchy.wins AS wins,\n\tbv_box_well.completionnumber_0 AS completion_number,\n\tbv_box_hierarchy.level1id_0 AS level_1_id,\n\tbv_box_hierarchy.level1description_0 AS level_1_description,\n\tbv_box_hierarchy.level2id_0 AS level_2_id,\n\tbv_box_hierarchy.level2description_0 AS level_2_description,\n\tbv_box_hierarchy.level3id_0 AS level_3_id,\n\tbv_box_hierarchy.level3description_0 AS level_3_description,\n\tbv_box_hierarchy.level4id_0 AS level_4_id,\n\tbv_box_hierarchy.level4description_0 AS level_4_description,\n\tbv_box_hierarchy.level5id_0 AS level_5_id,\n\tbv_box_hierarchy.level5description_0 AS level_5_description,\n\tbv_box_hierarchy.level6id_0 AS level_6_id,\n\tbv_box_hierarchy.level6description_0 AS level_6_description,\n\tbv_box_hierarchy.level7id_0 AS level_7_id,\n\tbv_box_hierarchy.level7description_0 AS level_7_description,\n\tbv_box_hierarchy.level8id_0 AS level_8_id,\n\tbv_box_hierarchy.level8description_0 AS level_8_description,\n\tbv_box_hierarchy.level9id_0 AS level_9_id,\n\tbv_box_hierarchy.level9description_0 AS level_9_description,\n\tbv_box_hierarchy.level10id_0 AS level_10_id,\n\tbv_box_hierarchy.level10description_0 AS level_10_description,\n\tbv_box_hierarchy.level11id_0 AS level_11_id,\n\tbv_box_hierarchy.level11description_0 AS level_11_description,\n\tbv_box_hierarchy.level12id_0 AS level_12_id,\n\tbv_box_hierarchy.level12description_0 AS level_12_description,\n\tbv_box_hierarchy.level13id_0 AS level_13_id,\n\tbv_box_hierarchy.level13description_0 AS level_13_description,\n\tbv_box_hierarchy.level14id_0 AS level_14_id,\n\tbv_box_hierarchy.level14description_0 AS level_14_description,\n\tbv_box_hierarchy.level15id_0 AS level_15_id,\n\tbv_box_hierarchy.level15description_0 AS level_15_description,\n\tbv_box_hierarchy.level16id_0 AS level_16_id,\n\tbv_box_hierarchy.level16description_0 AS level_16_description,\n\tbv_box_hierarchy.level17id_0 AS level_17_id,\n\tbv_box_hierarchy.level17description_0 AS level_17_description,\n\tbv_box_hierarchy.level18id_0 AS level_18_id,\n\tbv_box_hierarchy.level18description_0 AS level_18_description,\n\tbv_box_hierarchy.level19id_0 AS level_19_id,\n\tbv_box_hierarchy.level19description_0 AS level_19_description,\n\tbv_box_hierarchy.level20id_0 AS level_20_id,\n\tbv_box_hierarchy.level20description_0 AS level_20_description,\n\tbv_box_hierarchy.path AS path\nFROM\n\tbv_box_hierarchy LEFT JOIN\n\tbv_box_well ON (bv_box_hierarchy.wins = bv_box_well.wins AND bv_box_hierarchy.id = bv_box_well.costcenter_0) \n\nCONTEXT ('formatted' = 'yes');\n\nDROP VIEW IF EXISTS dv_budget_area_lob_daily_prod_forecast CASCADE;\n\nCREATE VIEW dv_budget_area_lob_daily_prod_forecast FOLDER = '/03 - derived (transformation) views' DESCRIPTION = 'Production forecast by scenario for the budget area level (separate rows for each line of business).  This view is filtered to only show base and wedge lines of business, but there can be multiple entries for base in future years.'  AS\n\nSELECT\n\tscenario_year AS scenario_year,\n\tscenario_id AS scenario_id,\n\tscenario AS scenario,\n\tscenario_int AS scenario_int,\n\tprod_date AS prod_date,\n\tsegment_id AS segment_id,\n\tsegment AS segment,\n\tdivision_id AS division_id,\n\tdivision AS division,\n\tregion_id AS region_id,\n\tregion AS region,\n\tasset_group_id AS asset_group_id,\n\tasset_group AS asset_group,\n\tbudget_area_id_prefix AS budget_area_id_prefix,\n\tbudget_area_id AS budget_area_id,\n\tbudget_area AS budget_area,\n\tline_of_business_id AS line_of_business_id,\n\tline_of_business AS line_of_business,\n\t--Determine wedge status from line of business:\n\t\tCASE WHEN line_of_business_id = 'LOB-10WD'\n\t\t\tTHEN 'Wedge'\n\t\t\tELSE CASE WHEN line_of_business_id = 'LOB-10E'\n\t\t\t\tTHEN 'Base'\n\t\t\t\tELSE CASE WHEN scenario_year < GETYEAR(prod_date)\n\t\t\t\t\tTHEN CASE WHEN line_of_business_id = 'LOB-10WB-YR1' THEN 'Base' ELSE NULL END\n\t\t\t\t\tELSE NULL\n\t\t\t\t\tEND\n\t\t\t\tEND\n\t\t\tEND AS wedge_status,\n\toperator_status AS operator_status,\n\tnet_dry_boe_forecast AS net_dry_boe_forecast,\n\tgross_dry_boe_forecast AS gross_dry_boe_forecast,\n\tnet_wet_boe_forecast AS net_wet_boe_forecast,\n\tgross_wet_boe_forecast AS gross_wet_boe_forecast,\n\tnet_oil_and_condensate_forecast AS net_oil_and_condensate_forecast,\n\tgross_oil_and_condensate_forecast AS gross_oil_and_condensate_forecast,\n\tnet_ngl_forecast AS net_ngl_forecast,\n\tgross_ngl_forecast AS gross_ngl_forecast,\n\tnet_dry_gas_forecast AS net_dry_gas_forecast,\n\tgross_dry_gas_forecast AS gross_dry_gas_forecast,\n\tnet_wet_gas_forecast AS net_wet_gas_forecast,\n\tgross_wet_gas_forecast AS gross_wet_gas_forecast,\n\tgas_shrink AS gas_shrink,\n\tCASE WHEN net_ngl_forecast > 0 --Use the calculated value (do > 0 not IS NOT NULL because there was a negative value once)\n\t\tTHEN CASE WHEN net_wet_gas_forecast > 0\n\t\t\tTHEN net_ngl_forecast / net_wet_gas_forecast\n\t\t\tELSE CASE WHEN net_dry_gas_forecast > 0 --CBM is all methane so wet = dry for gas and BOE\n\t\t\t\tTHEN net_ngl_forecast / net_dry_gas_forecast\n\t\t\t\tELSE sap_ngl_yield\n\t\t\t\tEND\n\t\t\tEND\n\t\tELSE /*Use SAP value*/ sap_ngl_yield\n\t\tEND AS calculated_ngl_yield,\n\tsap_ngl_yield AS sap_ngl_yield,\n\toil_factor,\n\tboe_run_time_forecast AS boe_run_time_forecast,\n\tboe_down_time_forecast AS boe_down_time_forecast,\n\toil_run_time_forecast AS oil_run_time_forecast,\n\toil_down_time_forecast AS oil_down_time_forecast,\n\tgas_run_time_forecast AS gas_run_time_forecast,\n\tgas_down_time_forecast AS gas_down_time_forecast,\n\tsimops_dry_boe_lost_forecast AS simops_dry_boe_lost_forecast,\n\tsimops_wet_boe_lost_forecast AS simops_wet_boe_lost_forecast,\n\tsimops_oil_lost_forecast AS simops_oil_lost_forecast,\n\tsimops_wet_gas_lost_forecast AS simops_wet_gas_lost_forecast\nFROM\n\tbv_box_budget_area_daily_prod_forecast AS bv_box_budget_area_daily_prod_forecast\nWHERE --Only select base and wedge rows (wedge is duplicated out for YR1, YR2 so only get current wedge)\n\tline_of_business_id = 'LOB-10E' OR --Base (existing)\n\tline_of_business_id = 'LOB-10WB-YR1' OR --This is the base for future years (i.e. add this to base in 2017 for 2016 forecast years)\n\tline_of_business_id = 'LOB-10WD' --Wedge (current year)\n\nCONTEXT ('formatted' = 'yes');\n\nDROP VIEW IF EXISTS dv_latest_forecast CASCADE;\n\nCREATE VIEW dv_latest_forecast FOLDER = '/03 - derived (transformation) views' DESCRIPTION = 'Shows the latest forecast available for each date.  Note that if the forecast was created after the month was closed, the forecast value equals actuals because past dates are all actualized.'  AS\n\nSELECT\n\tprod_date AS prod_date,\n\tCAST('int',SUBSTRING(MAX(scenario_id),0,4)) AS latest_scenario_year, --Don't understand why you have to go from 0 to 4, but less than that doesn't work\n\tMAX(scenario_id) AS latest_scenario_id,\n\tCONCAT(CAST('int',SUBSTRING(MAX(scenario_id),5,7)),' and ',CAST('int',SUBSTRING(MAX(scenario_id),8,10))) AS latest_scenario\nFROM\n\tdv_budget_area_lob_daily_prod_forecast\nWHERE\n\tscenario <> 'Actuals' AND\n\tscenario <> 'Budget'\nGROUP BY\n\tprod_date CONTEXT ('formatted' = 'yes');\n\nDROP VIEW IF EXISTS dv_rolling_forecast CASCADE;\n\nCREATE VIEW dv_rolling_forecast FOLDER = '/03 - derived (transformation) views' DESCRIPTION = 'The rolling forecast is the latest true forecast per month (i.e. not actualized), as available.  For example, January''s rolling forecast is the 12 and 0, February is 1 and 11, March is 2 and 10, etc.'  AS\n\nSELECT\n\td.prod_date,\n\tCASE WHEN GETYEAR(d.prod_date) = d.year AND GETMONTH(d.prod_date) <= d.month\n\t\tTHEN --The current month is actualized so roll back to latest true forecast:\n\t\t\tCASE WHEN GETMONTH(d.prod_date) = 1\n\t\t\tTHEN CONCAT(d.year - 1,'.12.00') --Make January '12 and 0' not '0 and 12' (no '0 and 12' forecast in Essbase)\n\t\t\tELSE CONCAT(d.year,\n\t\t\t\tCASE WHEN GETMONTH(d.prod_date) - 1 < 10\n\t\t\t\t\tTHEN CONCAT('.0',GETMONTH(d.prod_date) - 1)\n\t\t\t\t\tELSE CONCAT('.',GETMONTH(d.prod_date) - 1)\n\t\t\t\t\tEND,\n\t\t\t\tCASE WHEN 13 - GETMONTH(d.prod_date) < 10\n\t\t\t\t\tTHEN CONCAT('.0',13 - GETMONTH(d.prod_date))\n\t\t\t\t\tELSE CONCAT('.',13 - GETMONTH(d.prod_date))\n\t\t\t\t\tEND)\n\t\t\tEND\n\t\tELSE --Final true forecast not available so use latest:\n\t\t\tlatest_scenario_id\n\t\tEND AS scenario_id,\n\tCASE WHEN GETYEAR(d.prod_date) = d.year AND GETMONTH(d.prod_date) <= d.month\n\t\tTHEN --The current month is actualized so roll back to latest true forecast:\n\t\t\tCASE WHEN GETMONTH(d.prod_date) = 1\n\t\t\t\tTHEN '12 and 0' --Make January '12 and 0' not '0 and 12' (no '0 and 12' forecast in Essbase)\n\t\t\t\tELSE CONCAT(GETMONTH(d.prod_date) - 1, ' and ', 13 - GETMONTH(d.prod_date))\n\t\t\t\tEND\n\t\tELSE --Final true forecast not available so use latest:\n\t\t\tlatest_scenario\n\t\tEND AS scenario,\n\tCASE WHEN GETYEAR(d.prod_date) = d.year AND GETMONTH(d.prod_date) <= d.month\n\t\tTHEN 'Closed'\n\t\tELSE NULL\n\t\tEND AS month_status\nFROM (\n\t\tSELECT\n\t\t\tprod_date,\n\t\t\tlatest_scenario_id,\n\t\t\tlatest_scenario,\n\t\t\tlatest_scenario_year AS year,\n\t\t\tCAST('int',SUBSTRING(latest_scenario_id,5,7)) AS month, --Don't know why you have to go from 5 to 7, but less than that doesn't work\n\t\t\tCAST('int',SUBSTRING(latest_scenario_id,8,10)) AS remainder --Same, span seems weird, but it works?!\n\t\tFROM dv_latest_forecast\n\t) d\n--ORDER BY d.prod_date\n\nCONTEXT ('formatted' = 'yes');\n\nDROP VIEW IF EXISTS dv_calendar CASCADE;\n\nCREATE VIEW dv_calendar FOLDER = '/03 - derived (transformation) views' DESCRIPTION = 'This view creates a calendar of all dates from various data sources to be used as a comprehensive calendar going back to the beginning of production in PDB all the way through future forecasts.'  AS  \n\nSELECT\n\tcalendar_date\nFROM (--This nested part is required because UNION in Denodo is duplicating rows\n\tSELECT *\n\tFROM\n\t\t(SELECT date AS calendar_date FROM bv_box_daily GROUP BY date) UNION\n\t\t(SELECT prod_date AS calendar_date FROM dv_rolling_forecast)\n\t)\nGROUP BY calendar_date\n\nCONTEXT ('formatted' = 'yes');\n\nDROP VIEW IF EXISTS dv_budget_area_all_dates CASCADE;\n\nCREATE VIEW dv_budget_area_all_dates FOLDER = '/03 - derived (transformation) views' DESCRIPTION = 'This view is a required starting point for later views in order to avoid using a LEFT OUTER JOIN because Denodo does NOT pass WHERE parameters if a LEFT OUTER JOIN is used.  Therefore, this simple table is used first and then the additional tables are just added with a LEFT JOIN to this comprehensive table.'  AS\n\nSELECT\n\tc.calendar_date AS prod_date,\n\th.segment_id AS segment_id,\n\th.segment AS segment,\n\th.division_id AS division_id,\n\th.division AS division,\n\th.region_id AS region_id,\n\th.region AS region,\n\th.asset_group_id AS asset_group_id,\n\th.asset_group AS asset_group,\n\th.cost_center_prefix AS budget_area_id_prefix,\n\th.cost_center AS budget_area_id,\n\th.cost_center_description AS budget_area,\n\th.cost_center_category AS operator_status\nFROM --Join h to r to get every cost center for every date (required starting point for other views):\n\tdv_box_hierarchy h LEFT JOIN\n\tdv_calendar c\nWHERE\n\th.cost_center_type = 'BUDGET'\n\nCONTEXT ('formatted' = 'yes');\n\nDROP VIEW IF EXISTS dv_budget_area_daily_prod_forecast CASCADE;\n\nCREATE VIEW dv_budget_area_daily_prod_forecast FOLDER = '/03 - derived (transformation) views' DESCRIPTION = 'Production forecast by scenario for the budget area level with only a signel entry for base and one for wedge.'  AS\n\nSELECT\n\tf.scenario_year,\n\tf.scenario_id,\n\tf.scenario,\n\tf.scenario_int,\n\tf.prod_date,\n\tf.segment_id,\n\tf.segment,\n\tf.division_id,\n\tf.division,\n\tf.region_id,\n\tf.region,\n\tf.asset_group_id,\n\tf.asset_group,\n\tf.budget_area_id_prefix,\n\tf.budget_area_id,\n\tf.budget_area,\n\tf.wedge_status,\n\tf.operator_status,\n\tSUM(f.net_dry_boe_forecast) AS net_dry_boe_forecast,\n\tSUM(f.gross_dry_boe_forecast) AS gross_dry_boe_forecast,\n\tSUM(f.net_wet_boe_forecast) AS net_wet_boe_forecast,\n\tSUM(f.gross_wet_boe_forecast) AS gross_wet_boe_forecast,\n\tSUM(f.net_oil_and_condensate_forecast) AS net_oil_and_condensate_forecast,\n\tSUM(f.gross_oil_and_condensate_forecast) AS gross_oil_and_condensate_forecast,\n\tSUM(f.net_ngl_forecast) AS net_ngl_forecast,\n\tSUM(f.gross_ngl_forecast) AS gross_ngl_forecast,\n\tSUM(f.net_dry_gas_forecast) AS net_dry_gas_forecast,\n\tSUM(f.gross_dry_gas_forecast) AS gross_dry_gas_forecast,\n\tSUM(f.net_wet_gas_forecast) AS net_wet_gas_forecast,\n\tSUM(f.gross_wet_gas_forecast) AS gross_wet_gas_forecast,\n\tp.gas_shrink,\n\tCASE WHEN SUM(f.net_ngl_forecast) > 0 --Use the calculated value (do > 0 not IS NOT NULL because there was a negative value once)\n\t\tTHEN CASE WHEN SUM(f.net_wet_gas_forecast) > 0\n\t\t\tTHEN SUM(f.net_ngl_forecast) / SUM(f.net_wet_gas_forecast)\n\t\t\tELSE CASE WHEN SUM(f.net_dry_gas_forecast) > 0 --CBM is all methane so wet = dry for gas and BOE\n\t\t\t\tTHEN SUM(f.net_ngl_forecast) / SUM(f.net_dry_gas_forecast)\n\t\t\t\tELSE /*Use SAP value*/ p.sap_ngl_yield\n\t\t\t\tEND\n\t\t\tEND\n\t\tELSE /*Use SAP value*/ p.sap_ngl_yield\n\t\tEND AS calculated_ngl_yield,\n\tp.sap_ngl_yield,\n\tp.oil_factor,\n\t--All these need to be moved to GROUP BY when SQL load procedure is updated....\n\tMAX(f.boe_run_time_forecast) AS boe_run_time_forecast,\n\tMAX(f.boe_down_time_forecast) AS boe_down_time_forecast,\n\tMAX(f.oil_run_time_forecast) AS oil_run_time_forecast,\n\tMAX(f.oil_down_time_forecast) AS oil_down_time_forecast,\n\tMAX(f.gas_run_time_forecast) AS gas_run_time_forecast,\n\tMAX(f.gas_down_time_forecast) AS gas_down_time_forecast,\n\tMAX(f.simops_dry_boe_lost_forecast) AS simops_dry_boe_lost_forecast,\n\tMAX(f.simops_wet_boe_lost_forecast) AS simops_wet_boe_lost_forecast,\n\tMAX(f.simops_oil_lost_forecast) AS simops_oil_lost_forecast,\n\tMAX(f.simops_wet_gas_lost_forecast) AS simops_wet_gas_lost_forecast\nFROM\n\tdv_budget_area_lob_daily_prod_forecast f LEFT JOIN\n\tbv_pdb_enhanced_dailyvol_est_convertd p ON (f.prod_date = p.daily_rdg_date AND f.budget_area_id_prefix = p.budget_area_id_prefix)\nGROUP BY\n\tf.scenario_year,\n\tf.scenario_id,\n\tf.scenario,\n\tf.scenario_int,\n\tf.prod_date,\n\tf.segment_id,\n\tf.segment,\n\tf.division_id,\n\tf.division,\n\tf.region_id,\n\tf.region,\n\tf.asset_group_id,\n\tf.asset_group,\n\tf.budget_area_id_prefix,\n\tf.budget_area_id,\n\tf.budget_area,\n\tf.wedge_status,\n\tf.operator_status,\n\tp.gas_shrink,\n\tp.sap_ngl_yield,\n\tp.oil_factor\n\nCONTEXT ('formatted' = 'yes');\n\nDROP VIEW IF EXISTS dv_budget_area_adjustment_factors CASCADE;\n\nCREATE VIEW dv_budget_area_adjustment_factors FOLDER = '/03 - derived (transformation) views' DESCRIPTION = 'This view provides different shrink and yield values from different sources.  The planning group loads shrink, yield, and oil adjustment factors to SAP.  However, because the yield uploaded to SAP is not necessarily the same as the yield used for the forecast values, the Planning team recommended to calculate yield as yield = NGL Forecast / Wet Gas Forecast to ensure consistency until the SAP values can be resolved.  This table provides both options.  The shrink, oil adjustment factor, and SAP yield are exactly as uploaded to SAP by Planning.  The calculated yield uses the recommended equation based on the ROLLING FORECAST VALUES.  If the calculated values are not available for any reason, the calculated column will display the SAP value in lieu of null. The oil and gas run time, down time, and simops is included here because it does not require any extra joins and used in the budget area downtime view.  Currently those values are only provided from Wattenberg and are therefore null in every other asset.\n\nNote: Planning equation recommendation by George Snyder and verified by Melissa Bailey (2016)' \n    PRIMARY KEY ( 'factor_date' , 'budget_area_id' , 'wedge_status' ) AS\n\nSELECT\n--NOTE: You cannot use CASE WHEN in a field that is filtered or Denodo will NOT pass the filter!\n--Therefore, must find an all inclusive starting point for JOINs and select fields from there.\n\tr.scenario,\n\tr.month_status,\n\tb.prod_date AS factor_date, --If this was CASE WHEN on s and f then Denodo would SELECT * dates and filter at the very end\n\tb.segment_id,\n\tb.segment,\n\tb.division_id,\n\tb.division,\n\tb.region_id,\n\tb.region,\n\tb.asset_group_id,\n\tb.asset_group,\n\tb.budget_area_id_prefix, --If this was CASE WHEN on s and f then Denodo would SELECT * IDs and filter at the very end\n\tb.budget_area_id,\n\tb.budget_area,\n\tf.wedge_status,\n\tb.operator_status,\n\ts.gas_shrink,\n\ts.sap_ngl_yield,\n\tCASE WHEN f.net_ngl_forecast > 0 --Use the calculated value (do > 0 not IS NOT NULL because there was a negative value once)\n\t\tTHEN CASE WHEN f.net_wet_gas_forecast > 0\n\t\t\tTHEN f.net_ngl_forecast / f.net_wet_gas_forecast\n\t\t\tELSE CASE WHEN f.net_dry_gas_forecast > 0 --CBM is all methane so wet = dry for gas and BOE\n\t\t\t\tTHEN f.net_ngl_forecast / f.net_dry_gas_forecast\n\t\t\t\tELSE s.sap_ngl_yield\n\t\t\t\tEND\n\t\t\tEND\n\t\tELSE /*Use SAP value*/ s.sap_ngl_yield\n\t\tEND AS calculated_ngl_yield,\n\ts.oil_factor,\n\ts.sap_author,\n\ts.sap_created_date,\n\ts.sap_editor,\n\ts.sap_edited_date,\n\tf.oil_run_time_forecast,\n\tf.oil_down_time_forecast,\n\tf.gas_run_time_forecast,\n\tf.gas_down_time_forecast,\n\tf.simops_oil_lost_forecast,\n\tf.simops_wet_gas_lost_forecast\nFROM\n\tdv_budget_area_all_dates b LEFT JOIN\n\tdv_rolling_forecast r ON (\n\t\tb.prod_date = r.prod_date\n\t) LEFT JOIN\n\t--NEED TO UPDATE THIS TO core.bv_pdb_enhanced_dailyvol_est_converted WHEN IT GETS ALL THE STUFF WORKING:\n\tbv_pdb_enhanced_dailyvol_est_convertd s ON (\n\t\tb.budget_area_id_prefix = s.budget_area_id_prefix AND\n\t\tb.prod_date = s.daily_rdg_date\n\t) LEFT JOIN\n\tdv_budget_area_daily_prod_forecast f ON (\n\t\tb.budget_area_id_prefix = f.budget_area_id_prefix AND\n\t\tb.prod_date = f.prod_date AND\n\t\tr.scenario = f.scenario\n\t)\nCONTEXT ('formatted' = 'yes');\n\nDROP VIEW IF EXISTS budget_area_downtime CASCADE;\n\nCREATE VIEW budget_area_downtime FOLDER = '/05 - final (presentation) views' DESCRIPTION = 'PDB downtime by budget area (one row per day per budget area per downtime reason).\n\nNotes:\n- ADJUSTED lost production results from reasons listed in the cmpl_downtime view.\n- Only Wattenberg supplies downtime forecast information, so all other assets are null.'  AS\n\n--OLD CODE:\n--SELECT bv_budget_area_downtime.date AS prod_date, bv_budget_area_downtime.segment AS segment, bv_budget_area_downtime.division AS division, bv_budget_area_downtime.region AS region, bv_budget_area_downtime.assetgroup_0 AS asset_group, bv_budget_area_downtime.budgetarea_0 AS budget_area, bv_budget_area_downtime.wedgestatus_0 AS wedge_status, bv_budget_area_downtime.operatorstatus_0 AS operator_status, bv_budget_area_downtime.category AS category, bv_budget_area_downtime.hoursdown_0 AS hours_down, bv_budget_area_downtime.netdryboelost_0 AS net_dry_boe_lost, bv_budget_area_downtime.netwetboelost_0 AS net_wet_boe_lost, bv_budget_area_downtime.netoillost_0 AS net_oil_lost, bv_budget_area_downtime.netwetgaslost_0 AS net_wet_gas_lost, bv_budget_area_downtime.netdrygaslost_0 AS net_dry_gas_lost, bv_budget_area_downtime.grossdryboelost_0 AS gross_dry_boe_lost, bv_budget_area_downtime.grosswetboelost_0 AS gross_wet_boe_lost, bv_budget_area_downtime.grossoillost_0 AS gross_oil_lost,